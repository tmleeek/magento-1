<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="mpdailydeal-top-link">
                <arguments>
                    <argument name="label" xsi:type="string">Daily Deals</argument>
                    <argument name="path" xsi:type="string">mpdailydeal</argument>
                </arguments>
            </block>
        </referenceBlock>        
        <referenceBlock name="footer_links">
            <block class="Magento\Framework\View\Element\Html\Link" name="mpdailydeal-link">
                <arguments>
                    <argument name="label" xsi:type="string">Daily Deals</argument>
                    <argument name="path" xsi:type="string">mpdailydeal</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>